{
  "name": "co-share-socketio",
  "version": "2.0.3",
  "description": "networking implementation using socketio for co-share",
  "main": "dist",
  "dependencies": {
    "co-share": "^3.0.0",
    "react": "^17.0.1",
    "rxjs": "^6.6.6",
    "socket.io": "^3.1.2",
    "socket.io-client": "^3.1.2"
  },
  "devDependencies": {
    "@types/react": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "eslint": "^7.21.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "typescript": "^4.2.3"
  },
  "scripts": {
    "build": "tsc",
    "check": "run-s check:prettier check:eslint",
    "check:prettier": "prettier --check src/**/*.{ts,tsx}",
    "check:eslint": "eslint src",
    "fix": "run-s fix:prettier fix:eslint",
    "fix:prettier": "prettier --write src/**/*.{ts,tsx}",
    "fix:eslint": "eslint src --fix"
  }
}

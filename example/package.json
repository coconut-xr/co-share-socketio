{
    "name": "example",
    "version": "1.0.0",
    "scripts": {
        "build": "run-s build:server build:client",
        "build:server": "tsc -p tsconfig.json",
        "build:client": "webpack --config ./webpack.config.js",
        "start": "node dist/server",
        "start:development": "run-p start:development:server start:development:client",
        "start:development:server": "nodemon -e ts --watch src/counter-store.ts --watch src/server --exec npm run start:development:server:node",
        "start:development:server:node": "npm run build:server && node dist/server",
        "start:development:client": "webpack-dev-server --config ./webpack.config.js"
    },
    "dependencies": {
        "co-share-socketio": "../dist",
        "zustand": "^3.6.4"
    },
    "devDependencies": {
        "@types/react": "^17.0.34",
        "@types/react-dom": "^17.0.11",
        "nodemon": "^2.0.14",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.4.1",
        "ts-loader": "^9.2.6",
        "typescript": "^4.4.4",
        "webpack": "^5.62.1",
        "webpack-cli": "^4.9.1",
        "webpack-dev-server": "^4.4.0"
    }
}
